#!/bin/sh
# Script Information
export TESTCASE="F001_03"
export TEST_DESCRIPTION="Verify that duplicates cannot be loaded"
export TEST_PATH=__WDB_BUILDDIR__/test/install

# Test Environment
source $TEST_PATH/testEnvironment.sh

# Parse command line
while test -n "$1"; do
	case $1 in
	    --help) 
	    	echo "$SCRIPT_USAGE"; exit 0;;
	    --version) 
	    	echo "$0 $SCRIPT_VERSION"; exit 0;;
		--database=*)
		    WDB_DB=`echo $1 | sed 's/--database=//'`
		    shift
		    continue;;
		-d)
		    shift
		    WDB_DB=$1
		    shift
		    continue;;
		--user=*)
		    WDB_USER=`echo $1 | sed 's/--user=//'`
		    shift
		    continue;;
		-u)
		    shift
		    WDB_USER=$1
		    shift
		    continue;;
		--port=*)
		    WDB_PORT=`echo $1 | sed 's/--port=//'`
		    shift
		    continue;;
		-p)
		    shift
		    WDB_PORT=$1
		    shift
		    continue;;
	    --xml)
			XML_OPT="-x"
		    shift
		    continue;;
	    -x)
			XML_OPT="-x"
		    shift
		    continue;;
		*)
			NUM=$1
		    shift
		    continue;;
	esac
done

# DATABASE_NAME
if test -z "$WDB_DB"; then
    WDB_DB=$DEF_DB
fi
# DATABASE_USER
if test -z "$WDB_USER"; then
	WDB_USER=$DEF_USER
fi
# DATABASE_PORT
if test -z "$WDB_PORT"; then
	WDB_PORT=$DEF_PORT
fi
# DATABASE CONFIGURATION
export DB_CONF="-d $WDB_DB -u $WDB_USER -p $WDB_PORT"
export PSQL="psql -d $WDB_DB -U $WDB_USER -p $WDB_PORT"

# Timing
startTime=`date +%s%N`
# Prepare Test
TESTTIME=$(date +%FT12:00:00)
# Postgres>=8.3 this is duplicate key value violates...
# Postgres<=8.2 throw a different error message
EXPECTED_LOG_MESSAGE='duplicate key value violates unique constraint'
EXECUTE="${GRIBLOAD_PATH}/gribLoad --namespace=test ${DB_CONF} ${TEST_FILE}"

# Clean out database
source ${TEST_PATH}/tearDown.sh

# Generate Test Data
rm -f ${TEST_FILE}
${GRIBWRITE_PATH}/gribWrite --testGrid --generatingCentre 98 --generatingProcess 254 --parameter 151 --timeValue ${TESTTIME} --timeUnit 1 --timeRangeIndicator 0 --levelValue 0 --levelUnit 102 $TEST_FILE
${GRIBWRITE_PATH}/gribWrite --testGrid --generatingCentre 98 --generatingProcess 254 --parameter 130 --timeValue ${TESTTIME} --timeUnit 1 --timeRangeIndicator 0 --levelValue 0 --levelUnit 102 $TEST_FILE
${GRIBWRITE_PATH}/gribWrite --testGrid --generatingCentre 98 --generatingProcess 254 --parameter 168 --timeValue ${TESTTIME} --timeUnit 1 --timeRangeIndicator 0 --levelValue 0 --levelUnit 102 $TEST_FILE
${GRIBWRITE_PATH}/gribWrite --testGrid --generatingCentre 98 --generatingProcess 254 --parameter 168 --timeValue ${TESTTIME} --timeUnit 1 --timeRangeIndicator 0 --levelValue 0 --levelUnit 102 $TEST_FILE

#Execute Test 
$EXECUTE --logfile $LOGFILE
$EXECUTE --logfile $LOGFILE

# Check Result
TEST_RESULT=`grep -E "$EXPECTED_LOG_MESSAGE" $LOGFILE | wc -l`
if test ${TEST_RESULT} != "5"
then
	endTime=`date +%s%N`
	totalTime=$(echo "scale = 3; ($endTime-$startTime)/1000000000" | bc -l)
	if test "$XML_OPT" = "-x"; then
    	echo -e "<testcase name=\"`basename $0` $TEST_DESCRIPTION\" time=\"$totalTime\">\n\t<failure message=\"Did not find expected log message in logs\">\n\t</failure>\n</testcase>"
	else
    	echo -e "not ok $NUM - `basename $0`\t$TEST_DESCRIPTION"
	fi
    exit 1
fi

# Clean up
source ${TEST_PATH}/tearDown.sh

# Exit
endTime=`date +%s%N`
totalTime=$(echo "scale = 3; ($endTime-$startTime)/1000000000" | bc -l)
if test "$XML_OPT" = "-x"; then
	echo -e "<testcase name=\"`basename $0` $TEST_DESCRIPTION\" time=\"$totalTime\">\n</testcase>"
else
	echo -e "ok $NUM - `basename $0`\t$TEST_DESCRIPTION"
fi
exit 0
