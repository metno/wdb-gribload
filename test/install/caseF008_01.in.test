# Script Information
export TESTCASE="F008_01"
export TEST_DESCRIPTION="Data loaded in database matches data stored in file"
export TEST_PATH=__WDB_BUILDDIR__/test/install

# Test Environment
source $TEST_PATH/testEnvironment.sh

# Parse command line
case $1 in
    --help) 
    echo "$SCRIPT_USAGE"; exit 0;;
    --version) 
    echo "$0 $SCRIPT_VERSION"; exit 0;;
    --xml)
		XML_OPT="-x";;
    -x)
		XML_OPT="-x";;
esac

# Timing
startTime=`date +%s%N`
# Prepare Test
TESTTIME=$(date +%FT12:00)
EXECUTE="${GRIBLOAD_PATH}/gribLoad --name ${TEST_FILE}"
RESFILE="$TMP_PATH/case$TESTCASE.tmp"

# Clean out database
source ${TEST_PATH}/tearDown.sh

# Generate Test Data
rm -f ${TEST_FILE}
${GRIBWRITE_PATH}/gribWrite -P125,273=4.7 -P0,221=1.7 -P247,321=6.1 -P132,0=9.2 -P132,399=2.9 --hirlam10 --generatingCentre 1 --generatingProcess 252 --parameter 1 --timeValue ${TESTTIME} --timeUnit 1 --timeRangeIndicator 0 --levelValue 0 --levelUnit 102 ${TEST_FILE}

#Execute Test 
$EXECUTE --logfile $LOGFILE

# Check Log File
if grep -q -E "ERROR" $LOGFILE;
then
    # The log contains error messages
	endTime=`date +%s%N`
	totalTime=$(echo "scale = 3; ($endTime-$startTime)/1000000000" | bc -l)
	if test "$XML_OPT" = "-x"; then
    	echo -e "<testcase name=\"`basename $0` $TEST_DESCRIPTION\" time=\"$totalTime\">\n\t<failure message=\"There are error messages in the log\">\n\t</failure>\n</testcase>"
	else
    	echo -e "not ok $1 - `basename $0`\t$TEST_DESCRIPTION (log errors)"
	fi
    exit 1
fi
if grep -q -E "WARN" $LOGFILE;
then
    # The log contains warning messages
	endTime=`date +%s%N`
	totalTime=$(echo "scale = 3; ($endTime-$startTime)/1000000000" | bc -l)
	if test "$XML_OPT" = "-x"; then
    	echo -e "<testcase name=\"`basename $0` $TEST_DESCRIPTION\" time=\"$totalTime\">\n\t<failure message=\"There are warning messages in the log\">\n\t</failure>\n</testcase>"
	else
    	echo -e "not ok $1 - `basename $0`\t$TEST_DESCRIPTION (log warnings)"
	fi
    exit 1
fi


# Check Result
$PSQL -Atc "select wci.begin('gribtest'); select value from wci.read(ARRAY['test gribload'], 'nearest POINT(28.441 70.9346)', NULL, NULL, NULL, NULL, NULL, NULL::wci.returnFloat)" -o $RESFILE
if ! grep -qE "4.7" $RESFILE;
then
	endTime=`date +%s%N`
	totalTime=$(echo "scale = 3; ($endTime-$startTime)/1000000000" | bc -l)
	if test "$XML_OPT" = "-x"; then
    	echo -e "<testcase name=\"`basename $0` $TEST_DESCRIPTION\" time=\"$totalTime\">\n\t<failure message=\"Failure on point 1 check\">\n\t</failure>\n</testcase>"
	else
    	echo -e "not ok $1 - `basename $0`\t$TEST_DESCRIPTION (failure on point 1 check)"
	fi
	source ${TEST_PATH}/tearDown.sh
    exit 1
fi
$PSQL -Atc "select wci.begin('gribtest'); select value from wci.read(ARRAY['test gribload'], 'nearest POINT(-16.3113 75.7354)', NULL, NULL, NULL, NULL, NULL, NULL::wci.returnFloat)" -o $RESFILE
if ! grep -qE "1.7" $RESFILE;
then
	endTime=`date +%s%N`
	totalTime=$(echo "scale = 3; ($endTime-$startTime)/1000000000" | bc -l)
	if test "$XML_OPT" = "-x"; then
    	echo -e "<testcase name=\"`basename $0` $TEST_DESCRIPTION\" time=\"$totalTime\">\n\t<failure message=\"Failure on point 2 check\">\n\t</failure>\n</testcase>"
	else
    	echo -e "not ok $1 - `basename $0`\t$TEST_DESCRIPTION (failure on point 2 check)"
	fi
	source ${TEST_PATH}/tearDown.sh
    exit 1
fi
$PSQL -Atc "select wci.begin('gribtest'); select value from wci.read(ARRAY['test gribload'], 'nearest POINT(49.277 61.3378)', NULL, NULL, NULL, NULL, NULL, NULL::wci.returnFloat)" -o $RESFILE
if ! grep -qE "6.1" $RESFILE;
then
	endTime=`date +%s%N`
	totalTime=$(echo "scale = 3; ($endTime-$startTime)/1000000000" | bc -l)
	if test "$XML_OPT" = "-x"; then
    	echo -e "<testcase name=\"`basename $0` $TEST_DESCRIPTION\" time=\"$totalTime\">\n\t<failure message=\"Failure on point 3 check\">\n\t</failure>\n</testcase>"
	else
    	echo -e "not ok $1 - `basename $0`\t$TEST_DESCRIPTION (failure on point 3 check)"
	fi
	source ${TEST_PATH}/tearDown.sh
    exit 1
fi
$PSQL -Atc "select wci.begin('gribtest'); select value from wci.read(ARRAY['test gribload'], 'nearest POINT(-10.4419 50.1503)', NULL, NULL, NULL, NULL, NULL, NULL::wci.returnFloat)" -o $RESFILE
if ! grep -qE "9.2" $RESFILE;
then
	endTime=`date +%s%N`
	totalTime=$(echo "scale = 3; ($endTime-$startTime)/1000000000" | bc -l)
	if test "$XML_OPT" = "-x"; then
    	echo -e "<testcase name=\"`basename $0` $TEST_DESCRIPTION\" time=\"$totalTime\">\n\t<failure message=\"Failure on point 4 check\">\n\t</failure>\n</testcase>"
	else
    	echo -e "not ok $1 - `basename $0`\t$TEST_DESCRIPTION (failure on point 4 check)"
	fi
	source ${TEST_PATH}/tearDown.sh
    exit 1
fi
$PSQL -Atc "select wci.begin('gribtest'); select value from wci.read(ARRAY['test gribload'], 'nearest POINT(68.8721 72.1376)', NULL, NULL, NULL, NULL, NULL, NULL::wci.returnFloat)" -o $RESFILE
if ! grep -qE "2.8999" $RESFILE;
then
	endTime=`date +%s%N`
	totalTime=$(echo "scale = 3; ($endTime-$startTime)/1000000000" | bc -l)
	if test "$XML_OPT" = "-x"; then
    	echo -e "<testcase name=\"`basename $0` $TEST_DESCRIPTION\" time=\"$totalTime\">\n\t<failure message=\"Failure on point 5 check\">\n\t</failure>\n</testcase>"
	else
    	echo -e "not ok $1 - `basename $0`\t$TEST_DESCRIPTION (failure on point 5 check)"
	fi
	source ${TEST_PATH}/tearDown.sh
    exit 1
fi


# Clean up
source ${TEST_PATH}/tearDown.sh

# Exit
endTime=`date +%s%N`
totalTime=$(echo "scale = 3; ($endTime-$startTime)/1000000000" | bc -l)
if test "$XML_OPT" = "-x"; then
	echo -e "<testcase name=\"`basename $0` $TEST_DESCRIPTION\" time=\"$totalTime\">\n</testcase>"
else
	echo -e "ok $1 - `basename $0`\t$TEST_DESCRIPTION"
fi
exit 0
